<Window x:Class="AsheronBuilder.UI.AddEnvCellDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add EnvCell" Height="300" Width="400">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="EnvironmentListBox" Grid.Row="0"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
            <Button Content="OK" Width="60" Margin="0,0,10,0" Click="OkButton_Click"/>
            <Button Content="Cancel" Width="60" Click="CancelButton_Click"/>
        </StackPanel>
    </Grid>
</Window>